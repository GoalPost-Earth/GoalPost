    "@hookform/resolvers": "^3.9.1",
    "@neo4j/graphql": "^6.2.3",
    "@neo4j/graphql-ogm": "^5.9.1",
    "@parcel/watcher": "^2.4.1",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "apollo-server-micro": "^3.13.0",
    "body-parser": "^1.20.3",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.0",
